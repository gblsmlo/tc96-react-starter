{
	"name": "@tc96/react-starter",
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"start": "vite preview",
		"lint": "biome check --fix",
		"lint:staged": "biome check --staged --fix",
		"lint:ci": "biome ci",
		"prepare": "husky",
		"test": "echo 'No error found'",
		"type-check": "tsc --noEmit",
		"check:updates": "npm-check-updates -i --color"
	},
	"dependencies": {
		"@tailwindcss/vite": "^4.1.16",
		"@tc96/biome-config": "^2.0.0",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"tailwindcss": "^4.1.16"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.0",
		"@commitlint/cli": "20.1.0",
		"@commitlint/config-conventional": "20.0.0",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"@vitejs/plugin-react": "^5.1.0",
		"autoprefixer": "10.4.21",
		"globals": "^16.4.0",
		"husky": "9.1.7",
		"npm-check-updates": "^19.1.1",
		"typescript": "~5.9.3",
		"vite": "^7.1.12"
	},
	"lint-staged": {
		"src/**/*.{ts,tsx,js,jsx}": [
			"biome check --write"
		],
		"src/**/*.{json}": [
			"biome format --write"
		]
	},
	"sripts": {}
}
